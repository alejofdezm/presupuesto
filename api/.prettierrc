{
  "semi": false, 
  "singleQuote": true,
  "trailingComma": "all", 
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false, 
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxBracketSameLine": false, 
  "proseWrap": "never",
  "quoteProps": "as-needed", 
  "rangeStart": 0,
  "rangeEnd": Infinity,
  "requirePragma": false,
  "insertPragma": false,
  "jsxSingleQuote": false,
  "embeddedLanguageFormatting": "auto"
}